{
  "name": "qiji-pro",
  "version": "0.0.1",
  "private": true,
  "description": "qiji-pro",
  "bin": {
    "my-cli": "bin.js"
  },
  "scripts": {
    "start": "webpack serve --env   NODE_CONFIG=development --config webpack.dev.js --color --progress --hot",
    "start:prod": "webpack serve --env  NODE_CONFIG=production   --config  webpack.prod.js --color --progress --hot",
    "start:test": "webpack serve --env  NODE_CONFIG=test --config  webpack.prod.js --color --progress --hot",
    "build": "webpack --env  NODE_CONFIG=production --progress  --config  webpack.prod.js",
    "build-prod": "webpack --env   NODE_CONFIG=production --progress  --config  webpack.prod.js",
    "build-test": "webpack --env  NODE_CONFIG=test  --progress  --config  webpack.prod.js",
    "build-uat": "webpack --env  NODE_CONFIG=uat  --progress  --config  webpack.prod.js",
    "analyze": "-env NODE_ANALYZE=analyze npm run build",
    "stats": "webpack --progress --config webpack.prod.js  --json > stats.json",
    "mock": "cybmock start",
    "commit": "git-cz",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "lint-staged": "lint-staged",
    "lint-staged:js": "",
    "precommit1": "eslint src/**/*.{tsx,js}",
    "dep": "./beta.sh && npm run genlog",
    "genlog": "conventional-changelog -p custom-config -i CHANGELOG.md -s -r 0  -n ./changelog-option.js",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx  ./src",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx  ./src && npm run lint:style",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "lint:prettier": "check-prettier lint"
  },
  "config": {
    "validate-commit-msg": {},
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less lint:style",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "git add"
    ],
    "**/*.{js,jsx,ts,tsx}": "npm run lint:js"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "engines": {
    "npm": "6.14.10",
    "node": "14.15.4",
    "yarn": "1.10.1"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.7",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.2",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.7",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/react-resizable": "^3.0.2",
    "@types/react-slick": "^0.23.8",
    "@types/react-sticky": "^6.0.4",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.10.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "check-prettier": "^1.0.3",
    "clean-webpack-plugin": "^4.0.0",
    "commitizen": "^4.2.4",
    "compression-webpack-plugin": "^9.2.0",
    "conventional-changelog-cli": "^2.2.2",
    "conventional-changelog-custom-config": "^0.3.1",
    "copy-webpack-plugin": "^10.2.0",
    "core-js": "^3.20.2",
    "css-loader": "^6.5.1",
    "cssnano": "^5.0.15",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.7.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^3.1.0",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lint-staged": "^9.5.0",
    "lodash": "^4.17.21",
    "lodash-decorators": "^6.0.1",
    "mini-css-extract-plugin": "^1.6.2",
    "moment-locales-webpack-plugin": "^1.2.0",
    "postcss": "^8.4.8",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.2.3",
    "postcss-pxtorem": "^6.0.0",
    "sass": "^1.48.0",
    "sass-loader": "^12.4.0",
    "speed-measure-webpack-plugin": "^1.5.0",
    "style-loader": "^3.3.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "transform-class-properties": "^1.0.0-beta",
    "typescript": "^4.5.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.66.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@ant-design/charts": "^1.3.6",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-loadable": "^5.5.6",
    "@types/react-redux": "^7.1.22",
    "@types/react-router-dom": "^5.3.2",
    "@types/redux-logger": "^3.0.9",
    "amfe-flexible": "^2.2.1",
    "antd": "^4.22.8",
    "axios": "^0.25.0",
    "cross-env": "^7.0.3",
    "intersection-observer": "^0.12.0",
    "qiji": "^0.4.3",
    "qrcode.react": "^3.1.0",
    "rc-util": "^5.23.0",
    "react": "^17.0.2",
    "react-dnd": "^14.0.2",
    "react-dnd-html5-backend": "^14.0.0",
    "react-dom": "^17.0.2",
    "react-draggable": "^4.4.5",
    "react-images": "^1.2.0-beta.7",
    "react-intersection-observer": "^8.34.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.2.6",
    "react-resizable": "^3.0.4",
    "react-router-dom": "^5.3.3",
    "react-slick": "^0.28.1",
    "react-sticky": "^6.0.3",
    "redux": "^4.1.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.1",
    "slick-carousel": "^1.8.1"
  },
  "main": "bin.js"
}
